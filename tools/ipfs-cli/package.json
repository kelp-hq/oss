{
  "name": "@kelp_digital/ipfs-cli",
  "description": "ipfs-cli",
  "author": "did:substrate:5Fn9SNUE8LihCm7Lq5dpPgBebGy5D7ZKWESDsWbdjsfV37d4",
  "version": "0.0.1",
  "bin": {
    "ipfsCli": "dist/ipfsCli.js"
  },
  "keywords": [
    "ipfs",
    "auth",
    "gateway",
    "anagolay"
  ],
  "license": "GPL3",
  "type": "commonjs",
  "licenseFile": "./LICENSE",
  "main": "./lib/start.js",
  "types": "./lib/ipfs-cli.d.ts",
  "scripts": {
    "build:bin": "pkg --compress Brotli  --targets node17-linux ./dist/ipfsCli.js --out-path binaries",
    "build:all": "pnpm build && pnpm build:bin",
    "build": "pnpm build:prod",
    "build:prod": "NODE_ENV=production heft test --clean",
    "start": "heft start --clean",
    "watch": "heft build --clean --watch",
    "lint:fix": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "serve": "node ./lib/start.js",
    "link:bin": "sudo ln -sf $PWD/dist/ipfsCli.js /usr/local/bin/ipfsCli-dev && sudo chmod +x /usr/local/bin/ipfsCli-dev",
    "rm:bin": "sudo rm -f /usr/local/bin/ipfsCli-dev",
    "build:dev": "pnpm build && pnpm rm:bin && pnpm link:bin"
  },
  "dependencies": {
    "@anagolay/utils": "0.0.1",
    "ipfs-http-client": "56.0.0",
    "pino": "8.0.0",
    "commander": "9.2.0",
    "ramda": "0.28.0",
    "clui": "0.3.6",
    "byte-size": "8.1.0",
    "it-all": "1.0.6",
    "@sentry/node": "7.6.0",
    "@sentry/tracing": "7.6.0",
    "inquirer": "9.0.2",
    "yaml": "2.1.1",
    "undici": "5.6.1",
    "ipfs-core-types": "0.11.1",
    "node-fetch": "3.2.6"
  },
  "devDependencies": {
    "tslib": "2.4.0",
    "typescript": "4.6.4",
    "@rushstack/heft-node-rig": "1.9.7",
    "@microsoft/api-extractor": "7.28.0",
    "@rushstack/eslint-config": "2.6.0",
    "@rushstack/heft": "0.45.6",
    "@types/heft-jest": "1.0.2",
    "@types/node": "18.7.13",
    "eslint": "8.23.1",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "env-cmd": "10.1.0",
    "@types/inquirer": "8.2.0",
    "@types/clui": "0.3.1",
    "@rushstack/heft-webpack5-plugin": "0.5.13",
    "webpack-node-externals": "3.0.0",
    "@types/webpack-env": "1.16.3",
    "webpack": "5.68.0",
    "ts-loader": "9.2.6",
    "pino-pretty": "8.0.0",
    "prettier": "2.7.1",
    "webpack-cli": "4.9.2",
    "pkg": "5.6.0",
    "@types/ramda": "0.28.15",
    "@rushstack/heft-jest-plugin": "0.3.6",
    "@types/byte-size": "8.1.0"
  }
}
